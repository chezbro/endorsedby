<h1>Listing endorsements</h1>

<table>
  <thead>
    <tr>
      <th>Celeb</th>
      <th>Brand</th>
      <th>Amount</th>
      <th>Contract</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @endorsements.each do |endorsement| %>
      <tr>
        <td><%= endorsement.celeb_id %></td>
        <td><%= endorsement.brand %></td>
        <td><%= endorsement.amount %></td>
        <td><%= endorsement.contract %></td>
        <td><%= endorsement.image %></td>
        <td><%= link_to 'Show', endorsement %></td>
        <td><%= link_to 'Edit', edit_endorsement_path(endorsement) %></td>
        <td><%= link_to 'Destroy', endorsement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Endorsement', new_endorsement_path %>
